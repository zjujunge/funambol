<Script>
  <BeginTest><Arg>Sync19</Arg></BeginTest>
  <Include><Arg>../deleteall.xml</Arg></Include>
  <!-- Prepare the test environment -->
  <CreateEmptyEvent/>
  <SetEventField>
    <Arg>Summary</Arg>
    <Arg>Sync19_del</Arg>
  </SetEventField>
  <SetEventField>
    <Arg>Start</Arg>
    <Arg>20090113</Arg>
  </SetEventField>
  <SetEventField>
    <Arg>End</Arg>
    <Arg>20090114</Arg>
  </SetEventField>
  <SetEventField>
    <Arg>AllDay</Arg>
    <Arg>1</Arg>
  </SetEventField>
  <SaveEvent/>
  <CreateEmptyEvent/>
  <SetEventField>
    <Arg>Summary</Arg>
    <Arg>Sync19_upd</Arg>
  </SetEventField>
  <SetEventField>
    <Arg>Start</Arg>
    <Arg>20090113</Arg>
  </SetEventField>
  <SetEventField>
    <Arg>End</Arg>
    <Arg>20090114</Arg>
  </SetEventField>
  <SetEventField>
    <Arg>AllDay</Arg>
    <Arg>1</Arg>
  </SetEventField>
  <SaveEvent/>
  <!-- Synchronize -->
  <KeyPress>
    <Arg>KeyUp</Arg>
    <Arg> 5</Arg>
  </KeyPress>
  <KeyPress><Arg>KeyDown</Arg></KeyPress>
  <KeyPress><Arg>KeyFire</Arg></KeyPress>
  <WaitForSyncToComplete>
    <Arg>5</Arg>
    <Arg>120</Arg>
  </WaitForSyncToComplete>
  <CheckExchangedData>
    <Arg>Calendar</Arg>
    <Arg>2</Arg>
    <Arg>0</Arg>
    <Arg>0</Arg>
    <Arg>0</Arg>
    <Arg>0</Arg>
    <Arg>0</Arg>
  </CheckExchangedData>
  <!-- Now delete/upd/add -->
  <CreateEmptyEvent/>
  <SetEventField>
    <Arg>Summary</Arg>
    <Arg>Sync19_add</Arg>
  </SetEventField>
  <SetEventField>
    <Arg>Start</Arg>
    <Arg>20090113</Arg>
  </SetEventField>
  <SetEventField>
    <Arg>End</Arg>
    <Arg>20090114</Arg>
  </SetEventField>
  <SetEventField>
    <Arg>AllDay</Arg>
    <Arg>1</Arg>
  </SetEventField>
  <SaveEvent/>
  <LoadEvent><Arg>Sync19_upd</Arg></LoadEvent>
  <SetEventField>
    <Arg>Summary</Arg>
    <Arg>Sync19_upd2</Arg>
  </SetEventField>
  <SaveEvent/>
  <DeleteEvent><Arg>Sync19_del</Arg></DeleteEvent>
  <!--
     Synchronize
  -->
  <KeyPress>
    <Arg>KeyUp</Arg>
    <Arg> 5</Arg>
  </KeyPress>
  <KeyPress><Arg>KeyDown</Arg></KeyPress>
  <KeyPress><Arg>KeyFire</Arg></KeyPress>
  <WaitForSyncToComplete>
    <Arg>5</Arg>
    <Arg>120</Arg>
  </WaitForSyncToComplete>
  <CheckExchangedData>
    <Arg>Calendar</Arg>
    <Arg>1</Arg>
    <Arg>1</Arg>
    <Arg>1</Arg>
    <Arg>0</Arg>
    <Arg>0</Arg>
    <Arg>0</Arg>
  </CheckExchangedData>
  <EndTest/>
</Script>
