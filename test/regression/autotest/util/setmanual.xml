<Script>
<!-- setmanual.xml >
  
  <!-- Change settings to Manual -->
  <!-- Enter settings screen -->
  <KeyPress><Arg>KeyMenu</Arg></KeyPress>
  <KeyPress><Arg>KeyLeft</Arg></KeyPress>
  <KeyPress><Arg>KeyLeft</Arg></KeyPress>
  <KeyPress><Arg>KeyFire</Arg></KeyPress>
  <Wait><Arg>1</Arg></Wait>
  <!-- Find setting for Sync with server -->
  <KeyPress><Arg>KeyUp</Arg></KeyPress>
  <KeyPress><Arg>KeyUp</Arg></KeyPress>
  <KeyPress><Arg>KeyLeft</Arg></KeyPress>
  <KeyPress><Arg>KeyLeft</Arg></KeyPress>
  <KeyPress><Arg>KeyDown</Arg></KeyPress>
  <!-- Change setting to Scheduled -->
  <KeyPress><Arg>KeyFire</Arg></KeyPress>
  <Wait><Arg>1</Arg></Wait>
  <KeyPress><Arg>KeyUp</Arg></KeyPress>
  <KeyPress><Arg>KeyUp</Arg></KeyPress>
  <KeyPress><Arg>KeyFire</Arg></KeyPress>
  <!-- Save settings -->
  <KeyPress>
    <Arg>KeyDown</Arg>
    <Arg>15</Arg>
  </KeyPress>
  <KeyPress><Arg>KeyFire</Arg></KeyPress>

</Script>
