<Script>

  <!-- Change settings to Upload Older Videos -->
  <!-- Enter settings screen -->
  <KeyPress><Arg>KeyMenu</Arg></KeyPress>
  <KeyPress><Arg>KeyLeft</Arg></KeyPress>
  <KeyPress><Arg>KeyLeft</Arg></KeyPress>
  <KeyPress><Arg>KeyFire</Arg></KeyPress>

  <!-- Find Videos Sync Modes -->
  <KeyPress><Arg>KeyUp</Arg></KeyPress>
  <KeyPress><Arg>KeyUp</Arg></KeyPress>
  <KeyPress><Arg>KeyLeft</Arg></KeyPress>
  <KeyPress><Arg>KeyLeft</Arg></KeyPress>
  <KeyPress>
    <Arg>KeyDown</Arg>
    <Arg>9</Arg>
  </KeyPress>

  <!-- Toggle Videos Sync Mode to Upload Older Videos -->
  <KeyPress><Arg>KeyFire</Arg></KeyPress>
  
  <!-- Save settings -->
  <KeyPress>
    <Arg>KeyDown</Arg>
    <Arg>10</Arg>
  </KeyPress>
  <KeyPress><Arg>KeyFire</Arg></KeyPress>

</Script>
